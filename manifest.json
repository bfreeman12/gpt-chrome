{
  "manifest_version": 3,
  "name": "ChatGPT",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "action": {
    "default_popup": "main.html",
    "default_title": "ChatGPT"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"]
}
